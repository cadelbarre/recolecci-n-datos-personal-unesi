// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Personal {
  id             Int    @id @default(autoincrement())
  nombreCompleto String
  celular        String
  genero         String
  clinica        String
  otraClinica    String
  cargo          String
}
